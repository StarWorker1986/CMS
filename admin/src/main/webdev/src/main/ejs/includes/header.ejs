<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
<meta name="description" content="<%= i18n.description %>" />
<meta name="keywords" content="<%= i18n.keywords %>" />
<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0" />
<% 
	var asset = null, props = null;
	for(let i = 0; i < jsAssets.length; i++) {
	  props = [];
    asset = jsAssets[i];
    for(var item in asset) {
      props.push(item + '="' + asset[item] + '"');
    }
%>
<script type="text/javascript" <%- props.join(' ') %>></script>
<%
  }
  
  for(let i = 0; i < cssAssets.length; i++) {
	  props = [];
    asset = cssAssets[i];
    for(var item in asset) {
      props.push(item + '="' + asset[item] + '"');
  }
%>
<link rel="stylesheet" <%- props.join(' ') %> />
<%
  }
%>
